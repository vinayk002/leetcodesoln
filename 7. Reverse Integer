class Solution {
    public int reverse(int x) {

        int z,r;
        z=x;
        long n=0;
        while(z!=0)
        {
            r=z%10;
            n=n*10+r;
            z=z/10;

        }
        if (n<=Integer.MAX_VALUE && n>=Integer.MIN_VALUE)
        return (int)n;
        else
        return 0;
        
    }
}
